<script type="text/javascript">
    $(function () {
        var islogin = "";
        var isstate = "";
        var phone = "";
        var isSe = ""
        $.ajax({
            type: "post",
            url: "/api/v1/auth/login",
            contentType: "application/json;charset=utf-8",
            success: function (arry) {
                islogin = arry.result.logined;
                isstate = arry.result.qualified_level;
                phone = arry.result.phone;
							  isSe = arry.result.is_se;
                if(phone!=null&amp;&amp;phone.length&gt;0)
                {
                phone = phone.substring(0, 3) + "****" + phone.substring(7, 11);
                }
                if (islogin) {
                    $("#login").css("display", "none");
                    $("#regist").css("display", "none");
                    $("#slogin").css("display", "block");
                    $("#phone").html("您好！" + phone);
                }
            }, complete:

            function () {
							if(!isSe){
								showlogin(islogin, isstate);
							}

            }
        });
        $("#suser").toggle(function () {
            $("#duser").css("display", "none");
        }, function () {
            $("#duser").css("display", "block");
        });

        $("#topapp").toggle(function () {
            $("#imgtopapp").css("display", "none");
        }, function () {
            $("#imgtopapp").css("display", "block");
        });
        $("#topwx").toggle(function () {
            $("#imgtopwx").css("display", "none");
        }, function () {
            $("#imgtopwx").css("display", "block");
        });
        $("#botwx").toggle(function () {
            $("#imgbotwx").css("display", "none");
        }, function () {
            $("#imgbotwx").css("display", "block");
        });
        $("#botapp").toggle(function () {
            $("#imgbotapp").css("display", "none");
        }, function () {
            $("#imgbotapp").css("display", "block");
        });


        $(".user_center").hover(function () {
            var flag = $(this).hasClass('uc_active');
            if (flag) {
                $(this).removeClass('uc_active').find('.uc_bd').hide();
                $(this).find('.arrow').removeClass('arrow_up').addClass('arrow_down');
            } else {
                $(this).addClass('uc_active').find('.uc_bd').show();
                $(this).find('.arrow').addClass('arrow_up').removeClass('arrow_down');
            }
        });



        $("#review").click(function(){
            if (isstate == "0" || isstate == "-1"){
            window.location.href = "/licai/account/review?flag=setting&amp;next=" + location.href;
            }else{
                window.location.href = "/licai/account/result?reviewflag=setting&amp;grade="+isstate +"&amp;next="+ location.href;
            }
        });
        $("#mfpwd").click(function(){
            // window.location.href='/login/modifypassword.aspx?url='+location.href
            window.location.href='/licai/account/modifypassword?url='+location.href
        });
        $("#signout").click(function(){
            $.ajax({
                type: "get",
                url: "/api/v1/auth/logout",
                contentType: "application/json;charset=utf-8",
                success: function (data) {
                    window.location.href = location.href;
                }
            });
        });
    });

    function showlogin(islogin, isstate) {
        if (islogin) {
            document.getElementById("lgoin_new").style.display = "none";

            var state = isstate;
            if (state == "0" || state == "-1") {

                document.getElementById("weidenglu").style.display = "none";
                document.getElementById("weidajuan").style.display = "block";
                document.getElementById("pinghengxing").style.display = "none";
                window.setTimeout(setShowTime, 3000);
            }
            else if (state == "3") {
                document.getElementById("weidenglu").style.display = "none";
                document.getElementById("weidajuan").style.display = "none";
                document.getElementById("pinghengxing").style.display = "block";
                //window.setTimeout(setShowTime, 3000);
            }
            else {
                document.getElementById("tstk").style.display = "none";
                document.getElementById("beijing").style.display = "none";
            }

        } else {
            //document.getElementById("weidenglu").style.display = "block";
            //document.getElementById("weidajuan").style.display = "none";
            //document.getElementById("pinghengxing").style.display = "none";

            var url = window.location.pathname

            if(url === '/geshang/about.html' || url === '/geshang/qywh.html' || url === '/geshang/datebook.html' ||
              url === '/geshang/gltd.html' || url === '/geshang/ceo.html' ||
              url === '/geshang/kh.html' || url === '/geshang/recruitment.html' ||
              url === '/geshang/lxwm.html' || url === '/geshang/search.html')
            {

            	return
            }



            document.getElementById("lgoin_new").style.display = "block";
            //window.setTimeout(document.getElementById("login").style.display = "block", 3000);
            if(typeof ga !== "undefined"){
			          ga('send', 'event', "licai_pc_register", "register", "登录注册弹框弹出");
			      }
        }
    }
    function setShowTime() {
        document.getElementById("tstk").style.display = "block";
        document.getElementById("beijing").style.display = "block";
    }

